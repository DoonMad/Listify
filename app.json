{
  "expo": {
    "name": "Listify",
    "slug": "Listify",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/listify-icon.png",
    "scheme": "listify",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.doonmad.Listify",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/listify-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.doonmad.Listify"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/listify-icon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/listify-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "aa5a3955-e57d-41d3-90a6-40acd8567eea"
      }
    }
  }
}
